<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<HBox prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="DepressionDetection.GUIComponents.LoginPageController">
  <children>
    <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" style="#008CBA">
      <children>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Login " textAlignment="CENTER">
          <font>
            <Font name="System Bold" size="20.0" />
          </font>
        </Text>
        <TextField id="txtusername" maxWidth="-Infinity" prefHeight="30.0" prefWidth="200.0" promptText="Username">
          <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
          </VBox.margin>
        </TextField>
        <TextField id="txtpassword" maxWidth="-Infinity" prefHeight="30.0" prefWidth="200.0" promptText="Password">
          <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
          </VBox.margin>
        </TextField>
        <Button maxWidth="-1.0" mnemonicParsing="false" onAction="LoginPageController" text="Login" />
        <Label id="txtlabel" maxWidth="-Infinity" text="Status">
          <font>
            <Font size="24.0" />
          </font>
          <VBox.margin>
            <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
          </VBox.margin>
        </Label>
      </children>
    </VBox>
  </children>
</HBox>
